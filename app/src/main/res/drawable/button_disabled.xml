<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Drop Shadow Stack -->


    <!-- Background -->
    <item>
        <shape android:shape="rectangle">
            <stroke

                android:width="@dimen/dp_0.5"
                android:color="#ef939da0" />
            <corners android:radius="30dp" />
        </shape>
    </item>


    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1.5dp"
                android:color="#70939da0" />
            <corners android:radius="30dp" />

        </shape>
    </item>



    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1.5dp"
                android:color="#70939da0"  />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <corners android:radius="30dp" />
            <size android:height="@dimen/dp_50" />
            <corners android:radius="30dp" />
            <solid android:color="#FFA4AFB2" />
        </shape>
    </item>

</layer-list>



